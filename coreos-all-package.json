{
    "packer": {
        "description": "A simple CoreOS All for production.",
        "override_variables": {
            "name": "coreos-image",
            "release": "alpha",
            "version": "current",
            "build_path": "/packages/coreos-all-packer",
            "vagrant_box": "coreos-vagrant.box"
        },
        "list_variables": [
            "/packer-modules/variables/vars-global.json",
            "/packer-modules/variables/vars-coreos.json",
            "/packer-modules/variables/vars-machine-medium.json",
            "/packer-modules/variables/vars-vagrant.json",
            "/packer-modules/variables/vars-aws.json",
            "/packer-modules/variables/vars-azure.json",
            "/packer-modules/variables/vars-digitalocean.json",
            "/packer-modules/variables/vars-google.json"
        ],
        "builders": [
            "/packer-modules/builders/vagrant.json",
            "/packer-modules/builders/aws.json",
            "/packer-modules/builders/azure.json",
            "/packer-modules/builders/digitalocean.json",
            "/packer-modules/builders/google.json"
        ],
        "provisioners": [
            "/packer-modules/provisioners/shell-hello-world.json"
        ],
        "post_processors": [
            "/packer-modules/post-processors/vagrant-box.json",
            "/packer-modules/post-processors/vagrant-box-cloud.json"
        ],
        "min_packer_version": "1.1.3"
    }
}