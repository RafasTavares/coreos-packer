{
  "ignition": {
    "config": {},
    "timeouts": {},
    "version": "2.1.0"
  },
  "networkd": {},
  "passwd": {
    "users": [
      {
        "name": "core",
        "sshAuthorizedKeys": [
          "ssh-rsa  vagrant insecure public key"
        ]
      },
      {
        "name": "vagrant",
        "sshAuthorizedKeys": [
          "ssh-rsa  vagrant insecure public key"
        ]
      }
    ]
  },
  "storage": {
    "files": [
      {
        "filesystem": "root",
        "group": {
          "id": 0
        },
        "path": "/etc/sudoers.d/vagrant",
        "user": {
          "id": 0
        },
        "contents": {
          "source": "data:,vagrant%20ALL%3D(ALL)%20NOPASSWD%3A%20ALL",
          "verification": {}
        },
        "mode": 420
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nDescription=Install docker-compose\n\n[Service]\nType=oneshot\n\n# create /opt/bin directory\nExecStart=/usr/bin/mkdir -p /opt/bin/\n\n# sleep 2 seconds\nExecStart=/usr/bin/sleep 2\n\n# get docker-compose from github\n# -q quiet\n# -nc don't fetch if file already exists (return 0)\nExecStart=/bin/wget -q -nc -O /opt/bin/docker-compose \"https://github.com/docker/compose/releases/download/1.13.0/docker-compose-linux-x86_64\"\n\n# set execution rights\nExecStart=/usr/bin/chmod +x /opt/bin/docker-compose",
        "enable": true,
        "name": "install-docker-compose.service"
      }
    ]
  }
}
