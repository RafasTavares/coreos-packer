{ 
    "package": "coreos-alpha-template",
    "build_path": "./builds/packer-coreos-alpha", 
    "packer": {
        "description": "A simple CoreOS alpha for production.",
        "variables": {
            "name": "coreos",
            "release": "alpha",
            "version": "current",
            "build_path": "./builds/packer-coreos-alpha"
        },
        "list_variables": [
            "./packer/modules/variables/global.json",
            "./packer/modules/variables/coreos.json",
            "./packer/modules/variables/machine-large.json",
            "./packer/modules/variables/vagrant.json"
        ],
        "builders": [
            "./packer/modules/builders/vagrant.json"
        ],
        "provisioners": [
            "./packer/modules/provisioners/shell-hello-world.json"
        ],
        "post_processors": [
            "./packer/modules/post-processors/vagrant-box.json"
        ],
        "min_packer_version": "1.1.3"
    }
}